syntax = "proto3";

package proto.com.axiom.revbackend;

import "google/protobuf/timestamp.proto";
option java_multiple_files = true;

// serializing data from as per parameters
message FinRevenueBackend {
  /*
  offerName passed from json payload AXIOM
   */

  optional string offerName = 10;

  /*
  Product SKU from json payload for Pilot orders
   */
  optional string productName = 20;

  /*
  Contract number for which process is executed
   */
  optional string contractNumber = 30;

  // Reserved the old bufferDays field
  reserved 40;
  reserved "bufferDays";

  /*
  start date for json payload
   */
  reserved 41;
  reserved "fromDate";

  /*
  end date for json payload
  */
  reserved 45;
  reserved "toDate";

  /*
  Determines Execution Date
   */
  optional string execDate = 46;

  /*
  Determines draft vs final
   */
  optional string runMode = 50;

  /*
  output

  string responseMsg = 100;
  */
}